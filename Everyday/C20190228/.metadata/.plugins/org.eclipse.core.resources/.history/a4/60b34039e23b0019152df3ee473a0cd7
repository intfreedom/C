#include<stdio.h>
#define N 14
void main()
{
	int i,j,k,n=0,a[N][N];
	while(n<=0||n>=13){
		printf("Please input the line: ");
		scanf("%d", &n);
	}

	printf("%d line YangHui is: \n",n);
	for(i=1;i<=n;i++)
	{
		a[i][1]=a[i][i]=1;
	}

}


