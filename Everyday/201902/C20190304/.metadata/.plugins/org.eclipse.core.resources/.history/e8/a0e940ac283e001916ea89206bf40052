#include <stdio.h>
#include <malloc.h>
#include <stdlib.h>

#define LEN sizeof(struct student)

struct student *creat();
struct student *del(struct student *head, int num);

void print(struct student *head);

struct student
{
		int num;
		float score;
		struct student *next;
};

int n;//global varity, data;

void main()
{
	struct student *stu, *p;
	int n;

	stu = creat();
	p = stu;
	print(p);

	printf("Please enter the num to delete: ");
	scanf("%d",&n);
	printf(del(p,n));

	printf("\n\n");
	system("pause");
}
